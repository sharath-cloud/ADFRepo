{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Test2ADFSharath"
		},
		"LS_AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorage1'"
		},
		"LS_AzureDataLakeStorage1Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage1Gen2'"
		},
		"LS_AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase1'"
		},
		"linkedService1_StorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedService1_StorageGen2'"
		},
		"LS_AzureDataLakeStorage1Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://test1storagegen2sharath.dfs.core.windows.net"
		},
		"linkedService1_StorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://test1storagegen2sharath.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Test2ADFSharath_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureBlobStorage1_connectionString": {
						"value": "[parameters('LS_AzureBlobStorage1_connectionString')]"
					},
					"LS_AzureDataLakeStorage1Gen2_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage1Gen2_accountKey')]"
					},
					"LS_AzureSqlDatabase1_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase1_connectionString')]"
					},
					"linkedService1_StorageGen2_accountKey": {
						"value": "[parameters('linkedService1_StorageGen2_accountKey')]"
					},
					"LS_AzureDataLakeStorage1Gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage1Gen2_properties_typeProperties_url')]"
					},
					"linkedService1_StorageGen2_properties_typeProperties_url": {
						"value": "[parameters('linkedService1_StorageGen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}